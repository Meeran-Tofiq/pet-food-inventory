extends layout 

block content 
  div(class="animals-list with-background")
    ul
      each category in categoriesList
        li(class="animal-type")
          a(href=category.url + "/pet_foods")
            div(class="image-container" style=`background-image: url("${category.imageUrl}")`)
            p #{category.animal_type}
      li(class="create-new") 
        a(href="/animals/create") +